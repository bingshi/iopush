<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.stream.dao.IUserDao">
    <sql id="allAttribute">
        <![CDATA[

            id,
            account,
            userName,
            password,
            createDate,
            updateDate,
            isDel

        ]]>
    </sql>

    <insert id="save" parameterType="org.stream.entity.User" useGeneratedKeys="true" keyProperty="id">
        insert into user(account,userName,password,createDate)
        VALUES(#{account},#{userName},#{password},#{createDate})
    </insert>
</mapper>